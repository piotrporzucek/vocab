<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:my='urn:import:pl.egalit.vocab.client.login' xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:w="urn:import:com.google.gwt.widget.client">
	<ui:style>
		.panel {
			margin: 100px auto;
			text-align: left;
		}
		.label {
		font-size: x-large;
		font-weight: bold;
		margin-right: 50px;
		}


	</ui:style>

	<g:VerticalPanel styleName="{style.panel}">
		<g:HorizontalPanel>
			<g:Label text="Vokabes" styleName="{style.label}"></g:Label>

			<my:LoginComposite ui:field="loginComposite" />
		</g:HorizontalPanel>
	</g:VerticalPanel>

</ui:UiBinder>
